{
    "name": "@platforma-open/milaboratories.cellranger-genome-assets",
    "version": "1.0.0",
    "description": "CellRanger indexed genome references.",
    "scripts": {
        "cleanup": "rm -rf ./pkg-*.tgz && rm -rf ./dist/",
        "build": "pl-pkg build"
    },
    "block-software": {
        "entrypoints": {
