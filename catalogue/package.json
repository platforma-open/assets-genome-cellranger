            },
            "saccharomyces-cerevisiae": {
                "reference": "@platforma-open/milaboratories.cellranger-genome-assets.saccharomyces-cerevisiae/dist/tengo/asset/main.as.json"
            },
            "rattus-norvegicus": {
                "reference": "@platforma-open/milaboratories.cellranger-genome-assets.rattus-norvegicus/dist/tengo/asset/main.as.json"
            },
            "mus-musculus": {
                "reference": "@platforma-open/milaboratories.cellranger-genome-assets.mus-musculus/dist/tengo/asset/main.as.json"
            },
            "homo-sapiens": {
                "reference": "@platforma-open/milaboratories.cellranger-genome-assets.homo-sapiens/dist/tengo/asset/main.as.json"
