            },
            "bos-taurus": {
                "reference": "@platforma-open/milaboratories.cellranger-genome-assets.bos-taurus/dist/tengo/asset/main.as.json"
            },
            "arabidopsis-thaliana": {
                "reference": "@platforma-open/milaboratories.cellranger-genome-assets.arabidopsis-thaliana/dist/tengo/asset/main.as.json"
            }
        }
