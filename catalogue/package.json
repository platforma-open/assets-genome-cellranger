        "@platforma-open/milaboratories.cellranger-genome-assets.drosophila-melanogaster": "workspace:*",
        "@platforma-open/milaboratories.cellranger-genome-assets.danio-rerio": "workspace:*",
        "@platforma-open/milaboratories.cellranger-genome-assets.caenorhabditis-elegans": "workspace:*",
