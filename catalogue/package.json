        "@platforma-open/milaboratories.cellranger-genome-assets.saccharomyces-cerevisiae": "workspace:*",
        "@platforma-open/milaboratories.cellranger-genome-assets.rattus-norvegicus": "workspace:*",
        "@platforma-open/milaboratories.cellranger-genome-assets.mus-musculus": "workspace:*",
